{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://aura-backend-ecommerce-app.vercel.app/api/:path*"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://aura-backend-ecommerce-app.vercel.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}